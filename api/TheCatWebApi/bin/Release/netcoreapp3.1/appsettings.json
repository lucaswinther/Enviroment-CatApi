{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information"
    }
  },
  "AllowedHosts": "*",
  "ElasticApm": {
    "SecretToken": "",
    "ServerUrls": "http://localhost:8200",
    "ServiceName": "TheCatWebApi"
  },
  "ElasticSettings": {
    "BaseURL": "http://localhost:9200",
    "IndexName": "thecatlog",
    "DocMethod": "_doc"
  },
  "TheCatSettings": {
    "BaseURL": "https://api.thecatapi.com/v1/",
    "BreedsMethod": "breeds",
    "CategoryMethod": "categories",
    "ImageMethod": "images/search",
    "ImageCategoryFilter": [
      "hats",
      "sunglasses"
    ]
  },
  "ConnectionString": "Data Source=localhost;Initial Catalog=thecatdb;Connect Timeout=30;User Id=sa;Password=Your_password123"
}
